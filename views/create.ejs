<%- include("partials/nav")%>

<h1 class="m-3">Create Your Task </h1>

<form action="/create" method="POST">

    <input type="text" name="id" placeholder="Id" class="form-control w-25 m-3" >

    <input type="text" name="title" placeholder="Title" class="form-control w-25 m-3">

    <!-- <input type="text" name="description" placeholder="Description" class="form-control w-25 m-3"> -->

    <textarea name="description" placeholder="Description" class="form-control w-25 m-3" id="" cols="30" rows="10"></textarea>
    
    <input type="date" name="date" placeholder="dd/mm/yyyy"  class="form-control w-25 m-3">

    <button class="btn btn-success px-5 ms-3">Submit</button>

</form>
